---
title: My Blog
layout: 'base.njk'
---

   <section>
       <div class="wrap">
           <h1>Recent Post</h1>
           <div class="post">

              {%- for post in collections.post -%}

                  <div class="posts">
                         <h2><a href="{{post.url}}"> {{ post.data.title }} </a></h2>
                         <p> {{ post.data.description }} </p>
                  </div>

              {%- endfor -%}
              
           </div>
       </div>
   </section>


           
